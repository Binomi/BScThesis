	number = {2},
	urldate = {2016-02-04},
	journal = {Nature Photonics},
	author = {Goda, K. and Jalali, B.},
	month = feb,
	year = {2013},
	keywords = {Imaging and sensing, Optical physics},
	pages = {102--112},
	file = {Goda und Jalali - 2013 - Dispersive Fourier transformation for fast continu.pdf:C\:\\Users\\Felix\\Documents\\Zotero\\storage\\VVDC65QN\\Goda und Jalali - 2013 - Dispersive Fourier transformation for fast continu.pdf:;Snapshot:C\:\\Users\\Felix\\Documents\\Zotero\\storage\\4ZBA6PCR\\nphoton.2012.359.html:}
